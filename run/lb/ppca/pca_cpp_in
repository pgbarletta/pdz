parm ../../../top_files/dry_lb.prmtop
trajin ../data/full_fit_lb.nc
trajin ../data/ffull_fit_lb.nc
rms first :12-112@CA
average ffull_avg_lb.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference ffull_avg_lb.pdb [avg]
rms ref [avg] :12-112@CA RMSD
matrix mwcovar name mtx_lb :12-112@CA
createcrd crd_lb
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_lb vecs 297 out ffull_modes_lb
go
