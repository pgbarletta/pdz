!NVE run
&cntrl                                                                         
imin = 0,                           ! No Minimization                                                     
nmropt = 0,                         ! No NMR restraints read                                           
                                                                               
! Nature and format of the input                                               
ntx = 1,                            ! Coordinates will be read, assign new velocities
irest = 0,                          ! Do not run a new simulation                                        
                                                                               
! Nature and format of the output
ioutfm = 1,			    ! NetCDF format for output files
ntxo = 1,                           ! rstrt file in ASCII format
ntpr = 500,                         ! inf in output every ntpr steps                                   
ntwr = 500,                         ! rstrt file every ntwr steps                                     
ntwx = 50,                          ! mdcrd file every ntwx steps                                       
ntwv = 50,                          ! mdvel without velocities                                            
ntwe = 0,                           ! Energies and temperatures not written in mden                       
idecomp = 0,                        ! No energy decomposition                                          
iwrap = 1,                          ! Coordinates of the restart and mdcrd files wrapped into a primary b
                                                                               
! Frozen or restrained atoms                                                   
ibelly = 0,                         ! No frozen atoms                                                   
ntr=0,                              ! Harmonic potential restraints                                          
                                                                               
! Molecular dynamics                                                           
nstlim = 5000,                      ! Number of MD-steps                                            
dt = 0.002,                         ! Time step                                                         
                                                                               
! Temperature regulation                                                       
ntt = 0,                            ! Constant total energy
                                                                               
! P and V regulation                                                           
ntp = 0,                            ! No P control
ntb = 1,                            ! constant V
                                                                               
! SHAKE variables                                                              
ntc = 2,                            ! Bonds involving H are constrained                                    
ntf = 2,                            ! Bond interaction involving H are ommited.                            
                                                                               
cut = 9.0,                          ! Nonbonded cutoff 
/
&end  
