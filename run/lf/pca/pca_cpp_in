parm ../../../top_files/dry_lf.prmtop
trajin ../data/full_fit_lf.nc
rms first :12-112@CA
average full_avg_lf.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference full_avg_lf.pdb [avg]
rms ref [avg] :12-112@CA RMSD
matrix mwcovar name mtx_lf :12-112@CA
createcrd crd_lf
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_lf vecs 297 out full_modes_lf
go
